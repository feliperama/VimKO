{
  "lib/*.ex": {
    "alternate": "test/{}_test.exs",
    "template": [
      "defmodule {camelcase|capitalize|dot} do",
      "end"
    ],
    "type": "source"
  },
  "test/*_test.exs": {
    "alternate": "lib/{}.ex",
    "template": [
      "defmodule {camelcase|capitalize|dot}Test do",
      "",
      " alias {camelcase|capitalize|dot}",
      "end"
    ],
    "type": "test"
  },
  "src/*.ts" : {
    "alternate": "tests/unit/{}.test.ts",
    "type": "source"
  },
  "tests/unit/*.test.ts" : {
    "alternate" : "src/{}.ts",
    "type": "tests"
  }
}
